            e.preventDefault();
            e.stopPropagation();
            hideModal();
          });
          $(document).on("mouseleave", function() {
            if (sessionStorage.getItem("LightboxShown")) {return;}

            var negativeCondition = event.clientY <= 0 || event.clientX <= 0;
            var positiveCondition = event.clientX >= window.innerWidth || event.clientY >= window.innerHeight;
            if(positiveCondition || negativeCondition) {
              showModal();
              storeSession();
            }
          });
    
    
          // MOBILE
          var xDown = null;                                                        
    	  var yDown = null;

    	  function getTouches(evt) {
      	    return evt.originalEvent.touches;
    	  }    
    
          $(document).on("touchstart", function (evt) {
              var firstTouch = getTouches(evt)[0];
        	  xDown = firstTouch.clientX;                            
        	  yDown = firstTouch.clientY;
    	  });
   
    
          $(document).on("touchmove", function (evt) {
            if (sessionStorage.getItem("LightboxShown")) {return;}
            if (!xDown || !yDown) {
              return;
            }

            var xUp = evt.originalEvent.touches[0].clientX;                
            var yUp = evt.originalEvent.touches[0].clientY;

            var xDiff = xDown - xUp;
            var yDiff = yDown - yUp;  

            /* Swipe up */
            if (Math.abs(xDiff) <= Math.abs(yDiff) && yDiff <= 0) {
              showModal();
              storeSession();
            }
            /* reset values */
            xDown = null;
            yDown = null;
         });

        }
      });
