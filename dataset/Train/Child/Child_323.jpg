    var categoryLabelsTop = isTabletOrPhone ? 115 : 40;
  

    // Hide Header on on scroll down
    var didScroll;
    var lastScrollTop = 0;
    var delta = 0;
    var navbarHeight = objSearch.outerHeight();
  
    $(document).ready(function () {
      
    });

    $(window).scroll(function(event) {
      didScroll = true;
      
    });

    setInterval(function() {
      if (didScroll) {
        hasScrolled();
        didScroll = false;
        
      }
    }, 250);

    function hasScrolled() {
      var st = $(this).scrollTop();

      // Make sure they scroll more than delta
      if (Math.abs(lastScrollTop - st) <= delta)
        return;

      // If they scrolled down and are past the navbar, add class .closed.
      // This is necessary so you never see what is "behind" the navbar.
      if (st > lastScrollTop && st > navbarHeight) {
        // Scroll Down
        objSearch.addClass('closed');
      } else {
        // Scroll Up
        if (st + $(window).height() < $(document).height()) {
          objSearch.removeClass('closed');
        }
      }

      lastScrollTop = st;
    }

    objNav.on( 'mouseenter', function(){

      objSearch.removeClass( 'closed' );
      

    });

    objSearch.on( 'mouseleave', function(){

      if( lastScrollTop != 0 ) {

        objSearch.addClass( 'closed' );
        

      }

    });


